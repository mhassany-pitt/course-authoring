  "content": {
    "prompt": "Assertion Creation (1)",
    "source_code": "CREATE TABLE sailors\r\n\n   (sid INTEGER,\r\n\n    sname CHAR(10),\r\n\n    rating INTEGER,\r\n\n    age REAL,\r\n\n    PRIMARY KEY (sid),\r\n\n    CHECK\r\n\n    ((SELECT COUNT(S.sid) \r\n\n     FROM sailor S) + \r\n\n    (SELECT COUNT(B.sid) \r\n\n     FROM boats B)\r\n\n     < 100));\r\n"
  },
